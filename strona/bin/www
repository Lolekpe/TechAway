#!/usr/bin/env node
var debug = require('debug')('{name}');
var app, app_sprzed = require('../app');


app.set('port', 8080);
app_sprzed.set('port', 8082)

var server = app.listen(app.get('port'), function () {
  debug('Express server listening on port ' + server.address().port);
});
var server_sprzed = app_sprzed.listen(app_sprzed.get('port'), function () {
  debug('Express server listening on port ' + server_sprzed.address().port);
});